{
  "buildCommand": null,
  "outputDirectory": ".",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/((?!api/|media/|.*\\.(?:css|js|mp4|png|jpg|jpeg|svg|ico|mp3|webp|woff2?)$).*)",
      "destination": "/index.html"
    }
  ]
}